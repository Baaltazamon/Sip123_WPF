        <Button Content="-" HorizontalAlignment="Left" Margin="74,288,0,0" VerticalAlignment="Top" Width="23" Height="23" Click="Button_Click_3"/>
        <Button Content="*" HorizontalAlignment="Left" Margin="102,288,0,0" VerticalAlignment="Top" Width="23" Height="23" Click="Button_Click_4"/>
        <Button Content="/" HorizontalAlignment="Left" Margin="130,288,0,0" VerticalAlignment="Top" Width="23" Height="23" Click="Button_Click_5"/>
        <Button Content="%" HorizontalAlignment="Left" Margin="158,288,0,0" VerticalAlignment="Top" Width="23" Height="23" Click="Button_Click_6"/>
        <Button Content="Sin" HorizontalAlignment="Left" Margin="186,288,0,0" VerticalAlignment="Top" Width="23" Height="23" Click="Button_Click_7"/>
        <Button Content="C" HorizontalAlignment="Left" Margin="46,316,0,0" VerticalAlignment="Top" Width="51" Height="23" Click="Button_Click_8"/>
